<h1>Search Results</h1>

<h4>Prescriptions</h4>
	<table class="table table-striped table-hover table-bordered">
	  <thead class="table-info">
	    <tr>
	      <th>ID</th>
	      <th>Title</th>
	      <th>Description</th>
	    </tr>
	  </thead>

    	<tbody>
	    <% @prescriptions.each do | prescription | %>
	      <tr>
	        <td><%= prescription.id %></td>
	        <td><%= prescription.title %></td>
	        <td><%= prescription.description %></td>
	      </tr>
	    <% end %>
	  </tbody>
	</table>
	<br>

<h4>Users</h4>
<table class="table table-striped table-hover table-bordered">
	<thead class="table-danger">
		<tr>
			<th>First name</th>
			<th>Last name</th>
			<th>Phone</th>
			<th>Email</th>
			<th>Doctor</th>
			<th>Admin</th>
		</tr>
	</thead>

		<tbody>
		<% @users.each do | user | %>
			<tr>
				<td><%= user.first_name %></td>
				<td><%= user.last_name %></td>
				<td><%= user.phone %></td>
				<td><%= user.email %></td>
				<td><%= user.doctor %></td>
				<td><%= user.admin %></td>
			</tr>
		<% end %>
	</tbody>
</table>
<br>

<h4>Admins</h4>
<table class="table table-striped table-hover table-bordered">
	<thead class="table-danger">
		<tr>
			<th>First name</th>
			<th>Last name</th>
			<th>Phone</th>
			<th>Email</th>
			<th>Doctor</th>
			<th>Admin</th>
		</tr>
	</thead>

		<tbody>
		<% @admins.each do | admin | %>
			<tr>
				<td><%= admin.first_name %></td>
				<td><%= admin.last_name %></td>
				<td><%= admin.phone %></td>
				<td><%= admin.email %></td>
				<td><%= admin.doctor %></td>
				<td><%= admin.admin %></td>
			</tr>
		<% end %>
	</tbody>
</table>
<br>

	<h4>Doctors</h4>
	<table class="table table-striped table-hover table-bordered">
	  <thead class="table-primary">
	    <tr>
	      <th>First name</th>
	      <th>Last name</th>
	      <th>Phone</th>
	      <th>Email</th>
	      <th>Doctor</th>
	      <th>Admin</th>
	    </tr>
	  </thead>

    	<tbody>
	    <% @doctors.each do | user | %>
	      <tr>
	        <td><%= user.first_name %></td>
	        <td><%= user.last_name %></td>
	        <td><%= user.phone %></td>
	        <td><%= user.email %></td>
	        <td><%= user.doctor %></td>
	        <td><%= user.admin %></td>
	      </tr>
	    <% end %>
	  </tbody>
	</table>
	<br>

	